bird_colors <- function(palette_name="Scarlet Macaw", ncols = NA){
palette_name_gsub <- gsub("[- ']","_",palette_name)
pal_req <- bird_palettes[[palette_name_gsub]]
if(is.null(pal_req)){
stop(paste("Bird not supported. Options for palette are: ",cat(names(bird_palettes), sep = "\n"),sep=""))
}
if(is.na(ncols)){
ncols <- length(pal_req)
}
if (ncols > length(pal_req)){
stop("Bird isn't colorful enough. Ask for less colors or choose another bird.")
} else {
pal_req <- pal_req[1:ncols]
}
return(pal_req)
}
